{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": "off",
        "no-unused-vars": "off",
        "linebreak-style": "off",
        "no-restricted-globals" : "off",
        "no-use-before-define" : "off"
    }
}
